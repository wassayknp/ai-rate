{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "command": "npm install"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "APP_ENV": "preview"
      },
      "command": "npm install"
    },
    "production": {
      "autoIncrement": true,
            "channel": "production",
      "env": {
        "APP_ENV": "production"
      },
      "command": "npm install"
    }
  },
  "submit": {
    "production": {}
  }
}
